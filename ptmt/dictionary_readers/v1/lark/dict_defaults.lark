// For export
DICT_WORD: DICT_LETTER+
SPECIAL_CHARACTER: "@"|"="|"%"|"*"|"~"|"+"|"&"|"$"|UNICODE_MATHEMATICAL_OPERATORS|UNICODE_CURRENCY_SYMBOLS

_COLUMN_SEPARATOR: "|"
_ALTERNATIVE_SEPARATOR: ";"
_LANGUAGE_SEPARATOR: "\t"+ | ":"~2

PUNCTUATION_ALL: PUNCTUATION|UNICODE_GENERAL_PUNCTATION

MISCELLANEOUS_SYMBOLS: UNICODE_MISCELLANEOUS_SYMBOLS|UNICODE_MICELLANEOUS_TECHNICAL|UNICODE_IPA_EXTENSIONS|UNICODE_MODIFIER_TONE_LETTERS

LETTER_LIKE: UNICODE_LETTER_LIKE|UNICODE_DRAWING_ELEMENTS

SPECIAL_WORD: "."~3|"=>"

SEPARATOR: "\\"|"/"|","|":"|"#"

WS_INTERNAL: /[ \f\r\n\u00A0]/+

// Definitions
DICT_LETTER: (LETTER|UMLAUT|UCASE_LETTER|UCASE_UMLAUT|SPECIAL_WORD_COMPONENT|DIGIT|UNICODE)
UMLAUT: "ä"|"ö"|"ü"
UCASE_UMLAUT: "Ä"|"Ö"|"Ü"
SPECIAL_WORD_COMPONENT: "'"|"-"|"ʻ"|"^"|"ʿ"|"_"|/[u"\u0081"]/

PUNCTUATION: "!"|"?"|"."

UNICODE: UNICODE_EXTENDED_LATIN|UNICODE_GREEK_AND_COPTIC|UNICODE_SUPERSCRIPTS_AND_SUBSCRIPTS|UNICODE_COMBINING_DIACRITICAL_MARKS|UNICODE_ALL_OTHER_LANGUAGES|UNICODE_LANGUAGE_SYMBOLS|UNICODE_SPACING_MODIFIER_LETTERS|UNICODE_ALPHABETIC_PRESENTATION_FORMS
UNICODE_SPECIALS: /[u"\u0007\u007F"]/
UNICODE_EXTENDED_LATIN: /[u"\u00A1-\u024F\u1E00-\u1EFF\u2C60-\u2C7F\uA720-\uA7FF\uAB30-\uAB6F"]/
UNICODE_IPA_EXTENSIONS: /[u"\u0250-\u02AF"]/
