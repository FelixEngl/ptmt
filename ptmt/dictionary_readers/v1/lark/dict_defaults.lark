UNICODE_COMBINING_DIACRITICAL_MARKS: /[u"\u0300-\u036F"]/
UNICODE_GREEK_AND_COPTIC: /[u"\u0370-\u03FF\u1F00-\u1FFF"]/
UNICODE_ALL_OTHER_LANGUAGES: /[u"\u0400-\u1CCF"]/
UNICODE_GENERAL_PUNCTATION: /[u"\u2000-\u206F"]/
UNICODE_SUPERSCRIPTS_AND_SUBSCRIPTS: /[u"\u2070-\u209F"]/
UNICODE_CURRENCY_SYMBOLS: /[u"\u20A0-\u20CF"]/
UNICODE_LETTER_LIKE: /[u"\u2100-\u21FF"]/
